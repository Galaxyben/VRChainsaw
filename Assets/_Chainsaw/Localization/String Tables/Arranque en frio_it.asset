  m_Name: Arranque en frio_it
  m_EditorClassIdentifier: 
  m_LocaleId:
    m_Code: it
  m_SharedData: {fileID: 11400000, guid: a293abbca652725429353a685231c84c, type: 2}
  m_Metadata:
